{
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.4.1",
    "alex": "^11.0.0",
    "ava": "^5.3.0",
    "esbuild": "^0.18.1",
    "husky": "^8.0.2",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.2",
    "pretty-quick": "^3.1.3",
    "rollup": "^3.27.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^21.0.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "preci": "rm -rf node_modules",
    "ci": "yarn install",
    "reset": "rm -rf .git && git init",
    "setup": "npx husky install",
    "build": " rollup -c .config/rollup.config.js",
    "dev": "yarn build --watch",
    "lint": "alex .",
    "semantic-release": "semantic-release --branches release",
    "test": "NODE_OPTIONS='--loader=tsx' ava --verbose",
    "serve": "byndly --c .config/byndly.config.mjs"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ]
  },
  "name": "vay.js",
  "version": "1.0.0",
  "repository": "https://github.com/IamSebastianDev/vay.js.git",
  "author": "Sebastian <sebastian@iamsebastian.dev>",
  "unpkg": "./dist/index.browser.min.js"
}
